package course;

import java.util.Scanner;

public class Hello {

	public static void main(String[] args) {
		
		Scanner s = new Scanner(System.in);
		int di = s.nextInt();
		int df = s.nextInt();
		int duracao;
		
		if (di < df) {
			duracao = df - di;
		}
		
		else if (di > df) {
			duracao = 24 - di + df;
		}
		
		else {
			duracao = 24;
		}
		
		System.out.print("O jogo durou " + duracao + " hora(s)%n");
		
		s.close();
	}

}
