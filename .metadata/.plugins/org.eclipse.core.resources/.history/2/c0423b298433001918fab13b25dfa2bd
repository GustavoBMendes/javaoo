package entities;

public class Student {

	public String nome;
	public double nota1,
				  nota2,
				  nota3,
				  nfinal;
	
	public void pass() {
		nfinal = nota1 + nota2 + nota3; 
		if( nfinal > 60) {
			System.out.println("FINAL GRADE: " + nfinal);
			System.out.println("PASS");
		}
		else {
			System.out.println("FINAL GRADE: " + nfinal);
			System.out.println("FAILED");
			System.out.println("MISSING " + (60 - nfinal) + "POINTS");
		}
	}
	
}
